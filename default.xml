<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="github"
          fetch="https://github.com/" />
  <remote name="lineage"
          fetch="https://github.com/LineageOS" />
  <remote name="amazeos"
          fetch="https://github.com/amazeOS" />

  <default revision="lineage-21.0"
           remote="lineage"
           sync-j="4" />

  <!-- LineageOS repos -->
  <project path="build/make" name="android_build" groups="pdk" />
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" />
  <project path="frameworks/base" name="android_frameworks_base" groups="pdk-cw-fs,pdk-fs" />
  <project path="system/core" name="android_system_core" groups="pdk" />
  <project path="bionic" name="android_bionic" groups="pdk" />
  <project path="bootable/recovery" name="android_bootable_recovery" groups="pdk" />
  <project path="device/lineage/sepolicy" name="android_device_lineage_sepolicy" />
  <project path="hardware/lineage/interfaces" name="android_hardware_lineage_interfaces" />
  <project path="vendor/lineage" name="android_vendor_lineage" />

  <!-- amazeOS -->
  <project path="vendor/amazeOS" name="amazeOS" remote="amazeos" revision="main" />

</manifest>
